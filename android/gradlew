#!/usr/bin/env sh
# Minimal Gradle wrapper bootstrapper.
DIR=$(cd "$(dirname "$0")" && pwd)
GRADLE_WRAPPER_JAR="$DIR/gradle/wrapper/gradle-wrapper.jar"
if [ ! -f "$GRADLE_WRAPPER_JAR" ]; then
  echo "Gradle wrapper JAR missing. Please run 'gradle wrapper' to generate it." >&2
  exit 1
fi
exec java -Xmx64m -jar "$GRADLE_WRAPPER_JAR" "$@"
